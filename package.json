    "@preact/preset-vite": "^2.2.0",
    "@types/node": "^18.0.0",
    "sass": "^1.52.3",
    "tsc-watch": "^5.0.3",
    "typescript": "^4.7.4",
    "vite": "^2.9.12"
  }
